! special
*.foreground:   #00c5bb
*.background:   #15222f
*.cursorColor:  #00c5bb

! black
*.color0:       #0a0f14
*.color8:       #10151b

! red
*.color1:       #c33027
*.color9:       #9226b2

! green
*.color2:       #389abc
*.color10:      #cb1d1d

! yellow
*.color3:       #d3a245
*.color11:      #2b6475

! blue
*.color4:       #195465
*.color12:      #097eaa

! magenta
*.color5:       #4e5165
*.color13:      #888ba5

! cyan
